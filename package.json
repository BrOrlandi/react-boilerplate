{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint src",
    "prebuild": "rimraf dist",
    "build": "webpack -p --env.production",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrOrlandi/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Bruno Orlandi <brorlandi@gmail.com> (http://github.com/BrOrlandi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BrOrlandi/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/BrOrlandi/react-hot-boilerplate",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^2.0.0",
    "jest": "^19.0.2",
    "jest-faustao-errou": "^1.0.0",
    "node-sass": "^4.5.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^6.9.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/jest-faustao-errou/index.js"
  }
}
